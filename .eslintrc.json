{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-nested-ternary": "off",
    "max-classes-per-file": "off",
    "no-underscore-dangle": "warn",
    "new-cap": "warn",
    "import/extensions": [
      "error",
      {
        "js": "always",
        "json": "always"
      }
    ]
  }
}
