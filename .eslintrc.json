{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true
  },
  "globals": {
    "Highcharts": true,
    "L": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-nested-ternary": "off",
    "max-classes-per-file": "off",
    "no-underscore-dangle": "warn",
    "new-cap": "warn"
  }
}
